@model IEnumerable<Sotis2.Models.TmpAnsware>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.AnswareText)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.WasChecked)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.AttemptID)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.AnswareText)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.WasChecked)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.AttemptID)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.ID">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.ID">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.ID">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
